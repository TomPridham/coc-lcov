{
  "name": "coc-lcov",
  "version": "1.0.2",
  "description": "a coc plugin to provide lcov line markers",
  "author": "Tom Pridham <pridham.tom@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "coc.nvim"
  ],
  "engines": {
    "coc": "^0.0.80"
  },
  "scripts": {
    "lint": "eslint src --ext ts",
    "clean": "rimraf lib",
    "build": "tsc"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120,
    "semi": true
  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^10.12.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "coc.nvim": "^0.0.80",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.2"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "coc-lcov configuration",
      "properties": {
        "lcov.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable coc-lcov extension"
        },
        "lcov.uncoveredSign.text": {
          "type": "string",
          "default": "â–£",
          "description": "Sign to display on uncovered lines"
        },
        "lcov.uncoveredSign.hlGroup": {
          "type": "string",
          "default": "UncoveredLine",
          "description": "Uncovered sign color"
        },
        "lcov.jsonReportPath": {
          "type": "string",
          "default": "/coverage/coverage.info",
          "description": "Path to lcov report"
        }
      }
    }
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "lodash.debounce": "^4.0.8",
    "parse-lcov": "^1.0.4"
  }
}
